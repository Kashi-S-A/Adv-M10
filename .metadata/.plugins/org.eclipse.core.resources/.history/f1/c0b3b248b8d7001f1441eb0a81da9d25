package com.ty;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Student {

	@Id
	private int sid;

	private String name;

	private String email;

	// getter
	// setter

	@Override
	public String toString() {
		return "Student [sid=" + sid + ", name=" + name + 
				", email=" + email + "]";
	}
}
