public class Fetch {
	public static void main(String[] args) {
		EntityManagerFactory emf = Persistence.createEntityManagerFactory("onetomany");
		EntityManager em = emf.createEntityManager();

		College college = em.find(College.class, 101);
		
		
	}
}
